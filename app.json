{
  "expo": {
    "name": "CuppaZee",
    "slug": "PaperZee",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/CuppaZeeIcon.png",
    "splash": {
      "image": "./assets/CuppaZeeSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#00C35B"
    },
    "updates": {
      "fallbackToCacheTimeout": 30000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "uk.cuppazee.paper",
      "versionCode": 2,
      "icon": "./assets/CuppaZeeIcon.png",
      "adaptiveIcon": {
        "backgroundColor": "#00C35B",
        "foregroundImage": "./assets/CuppaZeeAndroid.png"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    }
  }
}
